{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "api/analyze.py"
    },
    {
      "src": "/api/projects",
      "dest": "api/auth.py"
    },
    {
      "src": "/api/projects/(.*)",
      "dest": "api/auth.py"
    },
    {
      "src": "/api/verify",
      "dest": "api/auth.py"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "api/reports.py"
    },
    {
      "src": "/api/developers",
      "dest": "api/reports.py"
    },
    {
      "src": "/api/developers/(.*)",
      "dest": "api/reports.py"
    },
    {
      "src": "/api/analyses/(.*)",
      "dest": "api/reports.py"
    }
  ]
}
